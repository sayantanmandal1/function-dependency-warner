{
  "name": "function-dependency-warner",
  "displayName": "Function Dependency Warner",
  "description": "Warns about code changes that affect dependent functions.",
  "version": "0.0.3",
  "publisher": "msaya",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "funcWarn.dependencyFile": {
          "type": "string",
          "description": "Path to the dependency JSON file",
          "default": "./dependencies.json"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc --watch -p ./",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin main"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.101.0",
    "@vscode/test-electron": "^2.3.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "function",
    "dependency",
    "code analysis",
    "warning",
    "typescript",
    "vscode-extension"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sayantanmandal1/function-dependency-warner"
  },
  "license": "MIT",
  "icon": "icon.jpeg"
}
